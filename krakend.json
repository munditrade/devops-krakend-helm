{
  "version": 3,
  "name": "My lovely gateway",
  "port": 8080,
  "cache_ttl": "3600s",
  "timeout": "3s",
  "extra_config": {
    "github_com/devopsfaith/krakend-gologging": {
      "level":  "DEBUG",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true
    }
  },
    "endpoints": [
        {
          "endpoint": "/health",
          "backend": [
            {
              "url_pattern": "/",
              "host": [
                "http://monitoring.prometheus-dev-cluster-server"
              ]
            }
          ]
        }
      ]
 }
